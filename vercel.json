{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "functions": {
        "dist/index.js": {
            "includeFiles": "dist/**"
        }
    }
}